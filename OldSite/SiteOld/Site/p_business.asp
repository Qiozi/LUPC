<%
	Set rs = conn.execute("SELECT id, business_title, business_img_url, business_content FROM tb_business  ")
	if not rs.eof then
		Response.write "<script>"
			do while not rs.eof
					Response.write "$('#business_title_"& rs("id") &"').html('"& JSescape(rs("business_title")) &"');"
					Response.write "$('#business_content_"& rs("id") &"').html('"& JSescape(rs("business_content")) &"');"
					Response.write "$('#business_img_"& rs("id") &"').attr('src', '"& rs("business_img_url") &"');"
			rs.movenext
			loop
		Response.write "</script>"
	End if
	rs.close : set rs = nothing

%>
